{
	"name": "terra-core",
	"private": true,
	"version": "0.1.0",
	"description": "terra-core",
	"engines": {
		"node": ">=8.9.2 <12"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cerner/terra-core.git"
	},
	"keywords": [
		"Cerner",
		"Terra",
		"Terra Core",
		"terra-core"
	],
	"author": "Cerner Corporation",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/cerner/terra-core/issues"
	},
	"browserslist": [
		"extends browserslist-config-terra"
	],
	"eslintConfig": {
		"extends": "terra",
		"overrides": [
			{
				"files": [
					"**/*/terra-dev-site/**/*.jsx"
				],
				"rules": {
					"import/no-extraneous-dependencies": "off",
					"import/no-unresolved": "off",
					"import/extensions": "off"
				}
			},
			{
				"files": [
					"*.doc.jsx"
				],
				"rules": {
					"import/no-webpack-loader-syntax": "off",
					"import/first": "off",
					"import/no-duplicates": "off"
				}
			}
		]
	},
	"stylelint": {
		"extends": "stylelint-config-terra",
		"rules": {
			"scss/at-mixin-pattern": "^(terra-)[a-z]+([a-z0-9-]+[a-z0-9]+)?$"
		}
	},
	"devDependencies": {
		"@babel/cli": "^7.5.0",
		"@babel/core": "^7.5.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.5.0",
		"@babel/plugin-transform-object-assign": "^7.2.0",
		"@babel/preset-env": "^7.5.0",
		"@babel/preset-react": "^7.0.0",
		"babel-jest": "^24.8.0",
		"browserslist-config-terra": "^1.0.0",
		"check-installed-dependencies": "^1.0.0",
		"core-js": "^3.1.3",
		"danger": "^9.1.0",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"enzyme-to-json": "^3.2.2",
		"eslint": "^6.1.0",
		"eslint-config-terra": "^3.0.0",
		"gh-pages": "^2.0.1",
		"glob": "^7.1.2",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^24.8.0",
		"lerna": "^3.16.4",
		"link-parent-bin": "^1.0.0",
		"raf": "^3.4.1",
		"react": "^16.8.5",
		"react-docgen": "^4.1.1",
		"react-dom": "^16.8.5",
		"react-test-renderer": "^16.5.2",
		"regenerator-runtime": "^0.13.2",
		"stylelint": "^11.0.0",
		"stylelint-config-terra": "^3.2.0",
		"terra-aggregate-translations": "^1.0.0",
		"terra-dev-site": "^6.5.0",
		"terra-disclosure-manager": "^4.9.0",
		"terra-enzyme-intl": "^3.0.0",
		"terra-toolkit": "^5.18.0",
		"webpack": "^4.30.0",
		"webpack-cli": "^3.3.2",
		"webpack-dev-server": "^3.3.1",
		"webpack-merge": "^4.1.2"
	},
	"dependencies": {
		"aphrodite": "^2.2.2",
		"classnames": "^2.2.5",
		"commander": "^2.9.0",
		"csvtojson": "^2.0.8",
		"focus-trap-react": "^6.0.0",
		"intl": "^1.2.5",
		"intl-locales-supported": "^1.8.4",
		"jsdom": "^15.1.1",
		"keycode-js": "^2.0.1",
		"lodash.uniqueid": "^4.0.1",
		"marked": "0.7.x",
		"mutationobserver-shim": "0.3.3",
		"object-fit-images": "^3.2.4",
		"prismjs": "~1.17.1",
		"prop-types": "^15.5.8",
		"react-animate-height": "^2.0.1",
		"react-lifecycles-compat": "^3.0.2",
		"react-portal": "^4.1.2",
		"react-syntax-highlighter": "^11.0.0",
		"resize-observer-polyfill": "^1.4.1",
		"shelljs": "^0.8.2",
		"svgo": "^1.2.2",
		"terra-hookshot": "^5.0.0",
		"wicg-inert": "^2.1.1"
	}
}
